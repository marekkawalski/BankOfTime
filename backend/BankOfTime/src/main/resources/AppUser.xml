<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.bankoftime.lol.AppUser">
        <table name="AppUser" schema="dbo" catalog="BankOfTime"/>
        <attributes>
            <id name="id">
                <column name="Id"/>
            </id>
            <basic name="name">
                <column name="Name"/>
            </basic>
            <basic name="lastName">
                <column name="LastName"/>
            </basic>
            <basic name="city">
                <column name="City"/>
            </basic>
            <basic name="country">
                <column name="Country"/>
            </basic>
            <basic name="username">
                <column name="Username"/>
            </basic>
            <basic name="password">
                <column name="Password"/>
            </basic>
            <basic name="phoneNumber">
                <column name="PhoneNumber"/>
            </basic>
            <basic name="userType">
                <column name="UserType"/>
            </basic>
            <basic name="locked">
                <column name="Locked"/>
            </basic>
            <basic name="enabled">
                <column name="Enabled"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
